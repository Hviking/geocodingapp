package com.beyondb.bean;


import com.beyondb.datasource.DataSource;


/**
 *
 * @author testnv
 */
public class TaskInfoBase {
    protected DataSource m_DataSource; 
    protected String m_info; //返回信息
    public final static  String info_Success = " 【成功】";
    public final static  String info_Failed  = " 【失败】";
    protected int m_totalcount; //成功个数
    protected String m_id; //任务标识
    /**
     *返回TiffTask执行后的数据源
     * @return
     */
    public DataSource getDataSource() {
        return m_DataSource;
    }

    

    public DataSource getDataBase()
    {
        return  m_DataSource;
    }
    /**
     * 返回描述性信息
     *
     * @return
     */
    public String getInfo() {
        return m_info;
    }

    /**
     * 返回成功的个数
     *
     * @return
     */
    public int getTotalCount() {
        return m_totalcount;
    }
       /**
     * 标识
     *
     * @return
     */
    public String getID() {
        return m_id;
    }
}
